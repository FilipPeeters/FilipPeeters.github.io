	function swapImage(which, currentImage) {
		$('#theImage').css('width', '');
		$('#theImage').attr('src', '/_images/'+which);
		$('#theImage').load(function() {
			getW=$('#theImage').width();
			getH=$('#theImage').height();
			///alert(getW);
			resizeImage();
		});
	}
	function gotoImage(theAmount) {
		thisAmount=currentImage+theAmount;
		if(thisAmount<0) {
			thisAmount=63-1;
		}
		if(thisAmount>=63) {
			thisAmount=0;
		}
		currentImage=thisAmount;
		$('#theImage').css('width', '');
		$('#theImage').attr('src', '/_images/'+imagesArray[thisAmount]);
		$('#theImage').load(function() {
			getW=$('#theImage').width();
			getH=$('#theImage').height();
			$('#movieTitle').html('<strong>'+imagesTextArray[thisAmount]+'</strong>');
			//alert(imagesTextArray[thisAmount]);
			resizeImage();
		});
	}
	
	
	function resizeImage() {
		myHeight=$(window).height();
		//myHeight2=myHeight;
		myWidth=$(window).width();
		myHeight=myHeight-175-72-50-30;
		//alert(myHeight2 +" "+ myHeight);
		
		getW=$('#theImage').width();
		getH=$('#theImage').height();
		//
		
		if(getW>0) {
					ratio=myHeight/getH;
			$('#theImage').height(myHeight);
			$('#theImage').width(ratio*getW);
			offset=175+myHeight+50;
			offset=175+myHeight+15;
			
		}
		
	}
$('#theImage').load(function() {
	//setTimeout("resizeImage();", 1000);
	resizeImage();
});	



	oktoscroll='false';
	function scrollToLeft() {
		if(oktoscroll=='true') {
			reel1Left=$('#reel1').position();
			reel1X=(reel1Left.left)+1;
			
			reel2Left=$('#reel2').position();
			reel2X=(reel2Left.left)+1;
			
			$('#reel1').css('left', reel1X+'px');
			if(reel2X > 40 ) {
				newX=reel2X-7031+40;
				$('#reel1').css('left', newX+'px');
			} else {
				$('#reel1').css('left', reel1X+'px');
			}
			
			if(reel1X > 40 ) {
				newX=reel1X-7031+40;
				$('#reel2').css('left', newX+'px');
			} else {
				$('#reel2').css('left', reel2X+'px');
			}
		}
	}
	
	function scrollToRight() {
		if(oktoscroll=='true') {
			reel1Left=$('#reel1').position();
			reel1X=(reel1Left.left)-1;
			
			reel2Left=$('#reel2').position();
			reel2X=(reel2Left.left)-1;
			
			$('#reel1').css('left', reel1X+'px');
			if(reel1X < (0-7031) ) {
				newX=reel2X+7031-40;
				$('#reel1').css('left', newX+'px');
			} else {
				$('#reel1').css('left', reel1X+'px');
			}
			
			if(reel2X < (0-7031) ) {
				newX=reel1X+7031-40;
				$('#reel2').css('left', newX+'px');
			} else {
				$('#reel2').css('left', reel2X+'px');
			}
		}
	}
	</script>

